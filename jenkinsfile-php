pipeline {
    agent any

    stages {
        stage("CHECKOUT REPO") {
            steps {
                echo "checkout repo"
            }
        }

        stage("BUILD PROJECT") {
            steps {
                echo "build project"
            }
        }

        stage("BUILD DOCKER IMAGE") {
            steps {
                echo "build docker image"
            }     
        }

        stage("PUSH DOCKER IMAGE") {
            steps {
                echo "push docker image"
            }
        }
    }
}