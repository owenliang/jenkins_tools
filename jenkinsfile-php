                echo "下载代码"
            }
        }

        stage("构建应用") {
            steps {
                echo "构建应用"
            }
        }

        stage("打包镜像") {
            steps {
                echo "打包镜像"
            }     
        }

        stage("推送镜像") {
            steps {
                echo "推送镜像"
            }
        }
    }
}